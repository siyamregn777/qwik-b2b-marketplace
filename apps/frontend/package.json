    "@qwikdev/pwa": "^0.0.4",
    "@types/node": "^20.0.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.1",
    "typescript": "5.7.3",
    "vite": "^6.2.6",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@auth/core": "^0.34.3",
    "graphql-request": "^7.1.2",
    "matrix-js-sdk": "^39.4.0",
